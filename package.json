  "scripts": {
    "ng": "ng",
    "start": "run-p client-ng2:live server:live client-pug:live",
    "build": "ng build",
    "test": "mocha",
    "lint": "ng lint",
    "e2e": "ng e2e",
    "postinstall": "ng build --aot -prod",
    "server:live": "nodemon --exec ./node_modules/.bin/ts-node -- ./server/index.ts",
    "client-ng2:live": "ng build --watch",
    "client-pug:live": "npm run pug-it:w",
    "pug-it": "pug src",
    "pug-it:w": "pug src -P -w"
  },
